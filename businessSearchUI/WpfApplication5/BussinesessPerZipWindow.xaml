<Window x:Class="WpfApplication5.BussinesessPerZipWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication5"
        xmlns:chartingToolkit="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        mc:Ignorable="d"
        Title="BussinesessPerZipWindow" Height="342" Width="1067.045">
    <Grid>
        <chartingToolkit:Chart x:Name="zipsChart" Title="Businesses Per Zipcode">
            <chartingToolkit:ColumnSeries DependentValuePath="Value"  IndependentValuePath="Key" ItemsSource="{Binding}" 
            IsSelectionEnabled="False"></chartingToolkit:ColumnSeries>
        </chartingToolkit:Chart>
    </Grid>
</Window>
